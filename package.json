{
  "name": "lowpolyschool",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "test": " node -e \"console.log('TESTS NOT IMPLEMENTED YET')\"",
    "clean": "run-p clean:*",
    "clean:docs": "rimraf docs",
    "clean:entrega": "rimraf entrega",
    "predev": "npm run clean",
    "dev": "vite",
    "prebuild": "npm run clean",
    "build": "vite build",
    "postbuild": "mkdir docs && mkdir entrega && ncp dist/ docs/ && rimraf dist && node scripts/adjust-imports.js && ncp docs/ entrega/",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "@types/three": "^0.134.0",
    "husky": "^7.0.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.7.1",
    "svelte": "^3.44.2",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.9.8",
    "sveltestrap": "^5.6.3",
    "three": "^0.134.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-html": "^2.1.1"
  }
}
